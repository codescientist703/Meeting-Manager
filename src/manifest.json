{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "homepage_url": "http://localhost:8080/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": ["<all_urls>", "*://*/*", "storage", "notifications", "tabs"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["js/content-script.js"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extensionName__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "commands": {
    "camera": {
      "suggested_key": "Alt+C",
      "description": "Mute/unmute the camera for a specified google meeting"
    },
    "mic": {
      "suggested_key": "Alt+M",
      "description": "Mute/unmute the microphone for a specified google meeting"
    },
    "sound": {
      "suggested_key": "Alt+S",
      "description": "Mute/unmute the sound for a specified google meeting"
    }
  }
}
